; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="tower_defense"
run/main_scene="res://window.tscn"
config/features=PackedStringArray("4.4", "C#", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

GameCoordinator="*res://scripts/GameManager/GameCoordinator.cs"
GameDataBase="*res://scripts/DataBase/GameDataBase.cs"
UITools="*res://scripts/UI/UITools.cs"

[display]

window/size/viewport_width=1440
window/size/viewport_height=810
window/size/mode=2
window/stretch/mode="canvas_items"
window/vsync/vsync_mode=0

[dotnet]

project/assembly_name="tower_defense"

[rendering]

2d/snap/snap_2d_transforms_to_pixel=true
2d/snap/snap_2d_vertices_to_pixel=true
